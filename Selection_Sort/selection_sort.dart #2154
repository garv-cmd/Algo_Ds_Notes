//selection sort 

import 'dart:io';

void selection_sort(List<int>arr,int n)
{
    int temp,min_index;
    
    for(int i=0;i<n-1;i++)
    {
        min_index=i;
        
        for(int j=i+1;j<n;j++)
            if(arr[j]<arr[min_index])
                min_index=j;
            
        
        if(i!=min_index)
        {
            temp=arr[i];
            arr[i]=arr[min_index];
            arr[min_index]=temp;
        }
    } 
}
void print_element(List<int>arr)
{
    for(int ele in arr)
        print(ele);
    
}

main()
{
    
    print("Enter the size of array ");
    var size=int.parse(stdin.readLineSync());
    
    List<int> array=List();
    
    print("Enter array element ");
    for(int i=0;i<size;i++)
    {
        var ele=int.parse(stdin.readLineSync());
        array.add(ele);
    }
   
    selection_sort(array,size);
     
    print("Sorted array is ");
    print_element(array);
    
     
 }
